#. type: Plain text
#: lia.1:4
msgid "lia - LDAP Inventory for Ansible"
msgstr ""

#. type: SH
#: lia.1:4 lia.json.5:4
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: lia.1:9
msgid "B<lia> [B<-h>] (B<--list>|B<--host> I<HOST>)"
msgstr ""

#. type: SH
#: lia.1:9 lia.json.5:6
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: lia.1:15
msgid ""
"B<lia> is a helper script to query hosts, groups, and variables from your "
"LDAP server, and provide it to B<ansible>(1).  B<lia> is highly configurable "
"to support caching and adjust to any LDAP schema."
msgstr ""

#. type: Plain text
#: lia.1:18
msgid ""
"The program will cache the inventory in JSON format, and serve it until the "
"cache expires."
msgstr ""

#. type: SH
#: lia.1:18
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: lia.1:19
#, no-wrap
msgid "B<-h>, B<--help>"
msgstr ""

#. type: Plain text
#: lia.1:22
msgid "Show usage information, and exit."
msgstr ""

#. type: TP
#: lia.1:22
#, no-wrap
msgid "B<--list>"
msgstr ""

#. type: Plain text
#: lia.1:25
msgid "Print all groups and variables."
msgstr ""

#. type: TP
#: lia.1:25
#, no-wrap
msgid "B<--host\\ >I<HOST>"
msgstr ""

#. type: Plain text
#: lia.1:28
msgid "Print variables for the host I<HOST>."
msgstr ""

#. type: SH
#: lia.1:28
#, no-wrap
msgid "EXIT STATUS"
msgstr ""

#. type: Plain text
#: lia.1:30
msgid "Returns zero if the inventory was retrieved."
msgstr ""

#. type: SH
#: lia.1:30
#, no-wrap
msgid "ENVIRONMENT"
msgstr ""

