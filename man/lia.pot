#. type: TH
#: lia.json.5:1
#, no-wrap
msgid "lia.json"
msgstr ""

#. type: Plain text
#: lia.json.5:4
msgid "lia.json - lia configuration file"
msgstr ""

#. type: Plain text
#: lia.json.5:9
msgid ""
"The JSON structure in this file defines how B<lia>(1) connects to LDAP "
"server, and what information it searches for."
msgstr ""

#. type: SH
#: lia.json.5:9
#, no-wrap
msgid "DICTIONARY MEMBERS"
msgstr ""

#. type: TP
#: lia.json.5:10
#, no-wrap
msgid "B<uri>"
msgstr ""

#. type: Plain text
#: lia.json.5:13
msgid "One or more space-delimited URIs of the LDAP server."
msgstr ""

#. type: TP
#: lia.json.5:13
#, no-wrap
msgid "B<page>"
msgstr ""

#. type: Plain text
#: lia.json.5:16
msgid "Optional. Page size to request for search operations."
msgstr ""

#. type: Plain text
#: lia.json.5:18
msgid "Default: 100."
msgstr ""

#. type: TP
#: lia.json.5:18
#, no-wrap
msgid "B<cache_time>"
msgstr ""

#. type: Plain text
#: lia.json.5:22
msgid ""
"Optional. Maximum age (in seconds) of the inventory cache at "
"I<$XDG_CACHE_HOME/lia.json> or I<~/.cache/lia.json>."
msgstr ""

#. type: Plain text
#: lia.json.5:24
msgid "Default: 10800."
msgstr ""

#. type: TP
#: lia.json.5:24
#, no-wrap
msgid "B<hosts>"
msgstr ""

#. type: Plain text
#: lia.json.5:28
msgid ""
"A dictionary defining how to search for hosts and which attributes to "
"request."
msgstr ""

#. type: TP
#: lia.json.5:28
#, no-wrap
msgid "B<groups>"
msgstr ""

#. type: Plain text
#: lia.json.5:32
msgid ""
"A list of dictionaries, each defining how to search for groups, which "
"attributes to request, and how to interpret host membership."
msgstr ""

#. type: SS
#: lia.json.5:32
#, no-wrap
msgid "Members of hosts dictionary"
msgstr ""

#. type: TP
#: lia.json.5:33
#, no-wrap
msgid "B<base>"
msgstr ""

#. type: Plain text
#: lia.json.5:36
msgid "The node in LDAP, from which the search starts."
msgstr ""

#. type: TP
#: lia.json.5:36
#, no-wrap
msgid "B<scope>"
msgstr ""

#. type: Plain text
#: lia.json.5:41
msgid ""
"Optional. LDAP search scope, may only be B<sub> for subtree search or any "
"other value for base search.  This is a limitation imposed by the ldap3 "
"library abstraction objects."
msgstr ""

#. type: Plain text
#: lia.json.5:43
msgid "Default: B<sub>."
msgstr ""

#. type: TP
#: lia.json.5:43
#, no-wrap
msgid "B<objectclass>"
msgstr ""

#. type: Plain text
#: lia.json.5:46
msgid "A list of object class names to search for."
msgstr ""

#. type: TP
#: lia.json.5:46 lia.json.5:102
#, no-wrap
msgid "B<attr>"
msgstr ""

#. type: Plain text
#: lia.json.5:50 lia.json.5:106
msgid ""
"A dictionary describing attributes to request. The following members are "
"required:"
msgstr ""

#. type: TP
#: lia.json.5:51
#, no-wrap
msgid "B<name>"
msgstr ""

#. type: Plain text
#: lia.json.5:54
msgid "The attribute containing host name (e.g. \"cn\")."
msgstr ""

#. type: Plain text
#: lia.json.5:58
msgid ""
"If multiple values are returned for a single object (in other words, a host "
"has multiple names), B<lia> will pick the one in common with the RDN "
"components."
msgstr ""

#. type: Plain text
#: lia.json.5:60
msgid "If none of the values are in the RDN, use the first alphabetically."
msgstr ""

#. type: Plain text
#: lia.json.5:62
msgid "For example:"
msgstr ""

#. type: TP
#: lia.json.5:63 lia.json.5:73 lia.json.5:83
#, no-wrap
msgid "LDAP object:"
msgstr ""

#. type: Plain text
#: lia.json.5:69
#, no-wrap
msgid ""
"dn: cn=charlie,dc=example,dc=net\n"
"cn: charlie\n"
"cn: alpha\n"
msgstr ""

#. type: TP
#: lia.json.5:70 lia.json.5:80 lia.json.5:90
#, no-wrap
msgid "Host name:"
msgstr ""

#. type: Plain text
#: lia.json.5:73
msgid "charlie"
msgstr ""

#. type: Plain text
#: lia.json.5:79
#, no-wrap
msgid ""
"dn: vCPU=2+cn=bravo,dc=example,dc=net\n"
"cn: bravo\n"
"cn: alpha\n"
msgstr ""

#. type: Plain text
#: lia.json.5:83
msgid "bravo"
msgstr ""

#. type: Plain text
#: lia.json.5:89
#, no-wrap
msgid ""
"dn: uuid=e035608b-cf04-4d2a-9451-0a7813c5225d,dc=example,dc=net\n"
"cn: bravo\n"
"cn: alpha\n"
msgstr ""

#. type: Plain text
#: lia.json.5:93
msgid "alpha"
msgstr ""

#. type: TP
#: lia.json.5:94
#, no-wrap
msgid "B<var>"
msgstr ""

#. type: Plain text
#: lia.json.5:97
msgid "The attribute that holds JSON-formatted variables."
msgstr ""

#. type: SS
#: lia.json.5:98
#, no-wrap
msgid "Members of each element in groups list"
msgstr ""

#. type: TP
#: lia.json.5:99
#, no-wrap
msgid "B<base>, B<scope>, B<objectclass>"
msgstr ""

#. type: Plain text
#: lia.json.5:102
msgid "Group search criteria. See B<Members of hosts dictionary> above."
msgstr ""

#. type: TP
#: lia.json.5:107
#, no-wrap
msgid "B<name>, B<var>"
msgstr ""

#. type: Plain text
#: lia.json.5:110
msgid "Identical to the eponymous B<hosts> members, see above."
msgstr ""

#. type: TP
#: lia.json.5:110
#, no-wrap
msgid "B<host>"
msgstr ""

#. type: Plain text
#: lia.json.5:113
msgid "The attribute listing member hosts."
msgstr ""

#. type: TP
#: lia.json.5:113
#, no-wrap
msgid "B<host_is_dn>"
msgstr ""

#. type: Plain text
#: lia.json.5:117
msgid ""
"If B<true>, then the attribute defined in B<host> lists distinguished names "
"of the member hosts. Otherwise, it lists host names."
msgstr ""

#. type: SH
#: lia.json.5:118
#, no-wrap
msgid "EXAMPLE CONFIGURATION"
msgstr ""

#. type: Plain text
#: lia.json.5:157
#, no-wrap
msgid ""
"{\n"
"  \"uri\": \"ldaps://ldap.example.org\",\n"
"  \"page\": 200,\n"
"  \"cache_time\": 86400,\n"
"  \"hosts\": {\n"
"    \"base\": \"ou=hosts,dc=example,dc=org\",\n"
"    \"scope\": \"sub\",\n"
"    \"objectclass\": [\"ansibleHost\", \"device\"],\n"
"    \"attr\": {\n"
"      \"name\": \"cn\",\n"
"      \"var\": \"ansibleVars\"\n"
"    }\n"
"  },\n"
"  \"groups\": [\n"
"    {\n"
"      \"base\": \"ou=ansible,ou=groups,dc=example,dc=org\",\n"
"      \"scope\": \"sub\",\n"
"      \"objectclass\": [\"groupOfNames\", \"ansibleGroup\"],\n"
"      \"attr\": {\n"
"        \"name\": \"cn\",\n"
"        \"host\": \"member\",\n"
"        \"host_is_dn\": true,\n"
"        \"var\": \"ansibleVars\"\n"
"      }\n"
"    },\n"
"    {\n"
"      \"base\": \"ou=hosts,dc=example,dc=org\",\n"
"      \"scope\": \"sub\",\n"
"      \"objectclass\": [\"organizationalUnit\", \"ansibleGroup\"],\n"
"      \"attr\": {\n"
"        \"name\": \"ou\",\n"
"        \"var\": \"ansibleVars\"\n"
"      }\n"
"    }\n"
"  ]\n"
"}\n"
msgstr ""

